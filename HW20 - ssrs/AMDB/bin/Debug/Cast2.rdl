  <DataSets>
    <DataSet Name="Cast">
      <Query>
        <DataSourceName>AMDB</DataSourceName>
        <CommandText>SELECT 
   p.Name,
   c.Character
  FROM Titles.Cast AS c 
  LEFT JOIN [AMDB].[Names].[Persons] AS p ON c.PersonId = p.PersonId
  WHERE MovieId = 858048</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Character">
          <DataField>Character</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Movies">
