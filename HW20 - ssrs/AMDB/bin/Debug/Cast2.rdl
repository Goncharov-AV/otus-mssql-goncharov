        <QueryParameters>
          <QueryParameter Name="MovieName">
            <Value>=Parameters!MovieName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT [MovieId]
      ,[Title]
     
  FROM [AMDB].[Titles].[Movies] WHERE Title LIKE N'@MovieName'</CommandText>
      </Query>
      <Fields>
