    <DataSet Name="Movie">
      <Query>
        <DataSourceName>AMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MovieId">
            <Value>=Parameters!MovieId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP (1) [MovieId]
      ,[ImdbMovieId]
      ,[Title]
      ,[OriginalTitle]
      ,[FullTitle]
      ,[MovieTypeId]
      ,[Year]
      ,[ReleaseDate]
      ,[RuntimeMins]
      ,[Plot]
      ,[ContentRatingId]
      ,[YearEnd]
      ,[BoxOfficeId]
  FROM [AMDB].[Titles].[Movies]
  WHERE MovieId = @MovieId</CommandText>
      </Query>
