          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Crew">
      <Query>
        <DataSourceName>AMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MovieId">
            <Value>=Parameters!MovieId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
       c.[TeamId]
      ,c.[MovieId]
      ,c.[PersonId]
	  ,p.Name
      ,c.[ProfessionId]
      ,c.[Job]
		,pr.Profession
  FROM [AMDB].[Titles].[Teams] AS c
LEFT JOIN [AMDB].[Names].[Persons] AS p ON c.PersonId = p.PersonId
LEFT JOIN [AMDB].[Dictionaries].[Professions] as pr on c.professionid = pr.professionid
 WHERE MovieId = @MovieId</CommandText>
      </Query>
