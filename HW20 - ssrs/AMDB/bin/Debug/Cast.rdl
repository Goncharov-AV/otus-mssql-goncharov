  <DataSets>
    <DataSet Name="Cast">
      <Query>
        <DataSourceName>AMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="MovieId">
            <Value>=Parameters!MovieId.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
   p.Name,
   c.Character
  FROM Titles.Cast AS c 
  LEFT JOIN [AMDB].[Names].[Persons] AS p ON c.PersonId = p.PersonId
  WHERE MovieId = @MovieId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Character">
          <DataField>Character</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Movies">
      <Query>
        <DataSourceName>AMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="MovieName">
            <Value>=Parameters!MovieName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 50 
[MovieId] ,
[Title]
FROM [AMDB].[Titles].[Movies] 
Where Year &gt; 2001
--WHERE Title LIKE N'@MovieName'</CommandText>
      </Query>
      <Fields>
        <Field Name="MovieId">
          <DataField>MovieId</DataField>
